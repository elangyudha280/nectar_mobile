{
  "expo": {
    "name": "nectar_mobile",
    "slug": "nectar_mobile",
    "version": "2.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#53B175"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#53B175"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Gilroy-Regular.ttf",
            "./assets/fonts/Gilroy-SemiBold.ttf",
            "./assets/fonts/Gilroy-Medium.ttf",
            "./assets/fonts/Gilroy-Ligth.ttf",
            "./assets/fonts/Gilroy-ExtraBold.ttf",
            "./assets/fonts/Gilroy-Bold.ttf",
            "./assets/fonts/Gilroy-Black.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
